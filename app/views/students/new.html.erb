<h1>Student Form</h1>

<%= form_tag students_path do %>

    <label>First name: </label>
    <%= text_field_tag :'student[first_name]' %><br>

    <label>Last Name: </label>
    <%= text_field_tag :'student[last_name]' %><br>

    <%= submit_tag "Submit Student Form" %>

<% end %>

<h1><%= @first_name %> <%= @last_name %> </h1>
